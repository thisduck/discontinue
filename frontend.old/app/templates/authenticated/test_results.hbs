<h2>Test Results</h2>
<table class="ui selectable celled table bg-white">
  <thead>
    <tr>
      <th>Build</th>
      <th>Box</th>
      <th>Description</th>
      <th>Test Type</th>
      <th>Location</th>
      <th>Duration</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |result|}}
      {{test-result result=result showBuild=true}}
    {{/each}}
  </tbody>
</table>
